<ul>
<% @users.each do |u| %>
  <li>
 	  <%= u.name %>
  <%= link_to 'Show', user_path(u) %>
  <%= link_to 'Edit', edit_user_registration_path(u) %>
  <%= link_to 'Delete', user_path(u), data: { confirm: "Are you sure?" }, method: :delete %>
  </li>
<% end %>
</ul>
<%= link_to 'Add New User', new_user_registration_path %>